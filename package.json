{
  "name": "met-web-next",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    ">0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "typescript-check": "npx tsc --noEmit --skipLibCheck --project tsconfig.json",
    "lint": "npx @biomejs/biome lint --write .",
    "generate-schema": "dotenv -c -- bash -c 'gql.tada generate schema https://graphql.datocms.com --header \"X-Exclude-Invalid: true\" --header \"Authorization: $DATOCMS_PUBLISHED_CONTENT_CDA_TOKEN\"'",
    "format": "npx @biomejs/biome format --write .",
    "prepare": "npm run generate-schema && cspell link add @cspell/dict-fr-fr && node jest/generateFragmentMocks.js",
    "lhci:mobile": "lhci autorun --config=lighthouse-config/mobile.js",
    "lhci:desktop": "lhci autorun --config=lighthouse-config/desktop.js",
    "postbuild": "cross-env NODE_OPTIONS='--import tsx' next-sitemap --config next-sitemap.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "spellcheck": "cspell \"**/*.{js,jsx,ts,tsx,md,json,yml,yaml}\"",
    "test": "jest --passWithNoTests",
    "generate:component": "cross-env NODE_OPTIONS='--import tsx' plop --plopfile scripts/plop/plopfile.ts component",
    "generate:block": "cross-env NODE_OPTIONS='--import tsx' plop --plopfile scripts/plop/plopfile.ts block"
  },
  "dependencies": {
    "@datocms/cda-client": "^0.2.6",
    "@datocms/cma-client": "^4.0.1",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typed-document-node": "^5.1.0",
    "@graphql-codegen/typescript": "^4.1.5",
    "@graphql-codegen/typescript-operations": "^4.5.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tailwindcss/postcss": "^4.0.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "datocms-structured-text-to-plain-text": "^5.0.0",
    "datocms-structured-text-utils": "^5.0.0",
    "gql.tada": "^1.8.10",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "jsdom": "^26.0.0",
    "next": "15.2.4",
    "next-intl": "^4.0.2",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.3",
    "react": "19.0.0",
    "react-datocms": "^7.2.2",
    "react-dom": "19.0.0",
    "serialize-error": "^12.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.15"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@chromatic-com/storybook": "^3.2.6",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@cspell/dict-fr-fr": "^2.2.5",
    "@lhci/cli": "^0.14.0",
    "@storybook/addon-essentials": "^8.6.9",
    "@storybook/addon-onboarding": "^8.6.9",
    "@storybook/blocks": "^8.6.9",
    "@storybook/experimental-addon-test": "^8.6.9",
    "@storybook/experimental-nextjs-vite": "^8.6.9",
    "@storybook/react": "^8.6.9",
    "@storybook/test": "^8.6.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitest/browser": "^3.0.9",
    "@vitest/coverage-v8": "^3.0.9",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "cspell": "^8.17.5",
    "deepmerge": "^4.3.1",
    "dotenv-cli": "^8.0.0",
    "fast-xml-parser": "^5.0.9",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lefthook": "^1.11.4",
    "playwright": "^1.51.1",
    "plop": "^4.0.1",
    "storybook": "^8.6.9",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  }
}
